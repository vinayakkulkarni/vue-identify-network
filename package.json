{
  "name": "vue-identify-network",
  "description": "Identify the network your users are using!",
  "version": "1.2.0",
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "vinayak.kulkarni@live.com",
    "url": "https://vinayak.pw"
  },
  "private": false,
  "main": "dist/vue-identify-network.min.js",
  "module": "dist/vue-identify-network.esm.js",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "vuejs",
    "detect",
    "internet-connectivity",
    "network-identification",
    "identify-2g",
    "identify-3g",
    "identify-4g"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinayakkulkarni/vue-identify-network.git"
  },
  "scripts": {
